{
    "name": "maxonfjvipon/pump-manager",
    "type": "project",
    "description": "Intelligent pump selector",
    "keywords": [
        "pump",
        "selection",
        "pump-manager"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "amrshawky/laravel-currency": "^4.0",
        "askedio/laravel-soft-cascade": "^8.1",
        "bkwld/cloner": "^3.9",
        "conedevelopment/i18n": "^1.1",
        "doctrine/dbal": "^3.2",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.5.4",
        "laravel-lang/lang": "~7.0",
        "laravel/framework": "^8.57",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "mariuzzo/laravel-js-localization": "^1.8",
        "mcamara/laravel-localization": "^1.6",
        "nwidart/laravel-modules": "^8.2",
        "rap2hpoutre/fast-excel": "^3.0",
        "sabberworm/php-css-parser": "^8.3",
        "spatie/laravel-multitenancy": "^1.0",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-translatable": "^4.6",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "tightenco/ziggy": "^1.3",
        "verumconsilium/laravel-browsershot": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
